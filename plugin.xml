<?xml version="1.0" encoding="UTF-8"?>



<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
           id="com.scotgov.ginsbergplugin"
      version="0.1.0">
    <name>GinsbergPlugin</name>


    <description>Echo demo plugin for Apache Cordova</description>
    <license>MIT</license>
    <keywords></keywords>
    <repo></repo>
    <issue></issue>

    <js-module src="www/ginsbergplugin.js" name="ginsbergplugin">
        <clobbers target="window.ginsbergplugin" />
    </js-module>

    <!-- windows8 -->
    <platform name="windows8">
        <js-module src="src/windows/ginsbergpluginProxy.js" name="GinsbergProxy">
            <merges target="" />
        </js-module>
        <!--<framework src="src/windows8/EchoRuntimeComponent.winmd" custom="true"/> -->
       <!--framework src="WinRTCproj/EchoRuntimeComponent.vcxproj" custom="true" type="projectReference"/-->
	   <framework src="src/windows/GinsbergPluginComponent/GinsbergPluginComponent.csproj" custom="true" type="projectReference"/>	   
    </platform>
        
</plugin>
